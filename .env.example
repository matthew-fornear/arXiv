# Copy to .env and replace placeholders with your browser session values.
ARXIV_COOKIE='arxiv_labs={...}; browser=...; arxiv-search-parameters="{\"order\": \"-announced_date_first\"\054 \"size\": \"50\"\054 \"abstracts\": \"show\"}"'
ARXIV_ACCEPT='text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8'
ARXIV_ACCEPT_LANGUAGE='en-US,en;q=0.9'
ARXIV_CACHE_CONTROL='max-age=0'
ARXIV_PRIORITY='u=0, i'
ARXIV_REFERER='https://arxiv.org/'
ARXIV_SEC_CH_UA='"Chromium";v="142", "Brave";v="142", "Not_A Brand";v="99"'
ARXIV_SEC_CH_UA_MOBILE='?0'
ARXIV_SEC_CH_UA_PLATFORM='"Linux"'
ARXIV_SEC_FETCH_DEST='document'
ARXIV_SEC_FETCH_MODE='navigate'
ARXIV_SEC_FETCH_SITE='cross-site'
ARXIV_SEC_FETCH_USER='?1'
ARXIV_SEC_GPC='1'
ARXIV_UPGRADE_INSECURE_REQUESTS='1'
ARXIV_USER_AGENT='Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/142.0.0.0 Safari/537.36'
